{
  "nodes": [
    {
      "id": "Azure_Token",
      "name": "Obter Token Azure",
      "type": "httpRequest",
      "parameters": {
        "url": "https://login.microsoftonline.com/{{TENANT_ID}}/oauth2/v2.0/token",
        "method": "POST",
        "responseFormat": "json",
        "jsonParameters": false,
        "options": {},
        "bodyParametersUi": {
          "parameter": [
            { "name": "client_id", "value": "{{CLIENT_ID}}" },
            { "name": "client_secret", "value": "{{CLIENT_SECRET}}" },
            { "name": "grant_type", "value": "client_credentials" },
            { "name": "scope", "value": "https://analysis.windows.net/powerbi/api/.default" }
          ]
        },
        "headerParametersUi": {
          "parameter": [
            { "name": "Content-Type", "value": "application/x-www-form-urlencoded" }
          ]
        }
      },
      "typeVersion": 1,
      "position": [200, 300]
    },
    {
      "id": "Export_PDF",
      "name": "Exportar PDF",
      "type": "httpRequest",
      "parameters": {
        "url": "https://api.powerbi.com/v1.0/myorg/groups/{{GROUP_ID}}/reports/{{REPORT_ID}}/ExportTo",
        "method": "POST",
        "responseFormat": "json",
        "jsonParameters": true,
        "options": {},
        "bodyParametersJson": "{\"format\": \"PDF\"}",
        "headerParametersJson": "={ \"Authorization\": \"Bearer {{$node[\\\"Obter Token Azure\\\"].json[\\\"access_token\\\"]}}\", \"Content-Type\": \"application/json\" }"
      },
      "typeVersion": 1,
      "position": [500, 300]
    },
    {
      "id": "Check_Status",
      "name": "Checar Status Export",
      "type": "httpRequest",
      "parameters": {
        "url": "=https://api.powerbi.com/v1.0/myorg/groups/{{GROUP_ID}}/reports/{{REPORT_ID}}/exports/{{$node[\"Exportar PDF\"].json[\"id\"]}}",
        "method": "GET",
        "responseFormat": "json",
        "headerParametersJson": "={ \"Authorization\": \"Bearer {{$node[\\\"Obter Token Azure\\\"].json[\\\"access_token\\\"]}}\" }"
      },
      "typeVersion": 1,
      "position": [800, 300]
    },
    {
      "id": "Wait_Export",
      "name": "Aguardar 10s",
      "type": "wait",
      "parameters": {
        "waitFor": "time",
        "value": 10,
        "unit": "seconds"
      },
      "typeVersion": 1,
      "position": [650, 450]
    },
    {
      "id": "Download_PDF",
      "name": "Baixar PDF",
      "type": "httpRequest",
      "parameters": {
        "url": "=https://api.powerbi.com/v1.0/myorg/groups/{{GROUP_ID}}/reports/{{REPORT_ID}}/exports/{{$node[\"Exportar PDF\"].json[\"id\"]}}/file",
        "method": "GET",
        "responseFormat": "file",
        "headerParametersJson": "={ \"Authorization\": \"Bearer {{$node[\\\"Obter Token Azure\\\"].json[\\\"access_token\\\"]}}\" }"
      },
      "typeVersion": 1,
      "position": [1100, 300]
    }
  ],
  "connections": {
    "Obter Token Azure": {
      "main": [[{ "node": "Exportar PDF", "type": "main", "index": 0 }]]
    },
    "Exportar PDF": {
      "main": [[{ "node": "Aguardar 10s", "type": "main", "index": 0 }]]
    },
    "Aguardar 10s": {
      "main": [[{ "node": "Checar Status Export", "type": "main", "index": 0 }]]
    },
    "Checar Status Export": {
      "main": [[{ "node": "Baixar PDF", "type": "main", "index": 0 }]]
    }
  }
}
